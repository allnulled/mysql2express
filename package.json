{
  "name": "mysql2express",
  "version": "1.0.0",
  "description": "Generate REST models and controllers automativally for Express web applications from a MySQL database.",
  "main": "src/index.js",
  "bin": {
    "m2e": "./bin/m2e"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "docs": "javadoc -i src/**/*.js -o docs/README.API.md -f markdown",
    "test": "mocha --sort test/*.test.js",
    "cover": "nyc --reporter=html mocha --sort test/*.test.js",
    "clean": "rimraf ./coverage ./.nyc_output && rimraf ./test/rest-1 && rimraf test/rest-2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allnulled/mysql2express.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allnulled/mysql2express/issues"
  },
  "homepage": "https://github.com/allnulled/mysql2express#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "execute-command-sync": "^1.0.2",
    "express": "^4.17.1",
    "javadoc": "^2.0.0",
    "knex": "^0.20.10",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "squel": "^5.13.0"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "mysql2": "^2.1.0",
    "yargs": "^15.1.0"
  }
}
