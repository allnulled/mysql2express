{
  "name": "mysql2express",
  "version": "1.0.0",
  "description": "Generate REST models and controllers automatically for Express web applications from a MySQL database.",
  "main": "src/index.js",
  "bin": {
    "mysql2express": "./bin/mysql2express"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "node dev/dev.js",
    "docs": "javadoc -i src/**/*.js -o docs/README.API.md -f markdown",
    "test": "mocha --sort test/*.test.js",
    "cover": "nyc --x \"./test/rest-1/**\" --reporter=html mocha --sort test/*.test.js",
    "clean": "rimraf ./coverage ./.nyc_output && rimraf ./test/rest-1 && rimraf test/rest-2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allnulled/mysql2express.git"
  },
  "keywords": [
    "rest",
    "webservice",
    "autogenerated",
    "generated",
    "automatic",
    "sql",
    "mysql",
    "database",
    "express",
    "models",
    "controllers"
  ],
  "author": "allnulled",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/allnulled/mysql2express/issues"
  },
  "homepage": "https://github.com/allnulled/mysql2express#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "dotenv": "^8.2.0",
    "execute-command-sync": "^1.0.2",
    "express": "^4.17.1",
    "globby": "^11.0.0",
    "javadoc": "^2.0.0",
    "knex": "^0.20.10",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "squel": "^5.13.0",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "mysql2": "^2.1.0",
    "yargs": "^15.1.0"
  }
}
